{
  "name": "tempo-calendar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:api": "dotenv -e .env.test -- jest api-gamification.test.ts",
    "test:api:verbose": "dotenv -e .env.test -- jest api-gamification.test.ts --verbose",
    "test:api:coverage": "dotenv -e .env.test -- jest api-gamification.test.ts --coverage",
    "db:backup": "bash scripts/backup-database.sh",
    "db:rollback": "bash scripts/rollback-migration.sh",
    "db:seed": "tsx prisma/seed-initial-data.ts",
    "db:seed:test": "dotenv -e .env.test -- tsx scripts/seed-test-profiles.ts",
    "db:seed:reset": "prisma migrate reset --force && npm run db:seed && npm run db:seed:test",
    "db:upgrade:user": "tsx scripts/upgrade-user-profile.ts",
    "test:db:push": "dotenv -e .env.test -- prisma db push --accept-data-loss"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@langchain/core": "^0.3.21",
    "@langchain/google-genai": "^0.1.5",
    "@langchain/langgraph": "^0.2.19",
    "@prisma/client": "^5.20.0",
    "better-sqlite3": "^12.5.0",
    "date-fns": "^3.0.0",
    "dotenv": "^17.2.3",
    "googleapis": "^166.0.0",
    "lucide-react": "^0.555.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.13",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.13.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^30.2.0",
    "postcss": "^8.4.0",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
}
